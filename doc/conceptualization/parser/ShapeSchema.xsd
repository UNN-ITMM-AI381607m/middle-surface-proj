<xs:schema elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:element name="Point">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="X" type="xs:double" />
                <xs:element name="Y" type="xs:double" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="BezierCurve" nillable="false">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="Point" minOccurs="2" maxOccurs="4" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="Shape2D">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="Contour" minOccurs="1" maxOccurs="unbounded" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="Contour">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="Points">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element ref="Point" minOccurs="1" maxOccurs="unbounded" />
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element name="Segments">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element ref="BezierCurve" minOccurs="1" maxOccurs="unbounded" />
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
</xs:schema>